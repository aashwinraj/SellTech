<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.selltech">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/profileicon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/profileicon_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SellTech">
        <activity android:name=".ui.CartActivity"></activity>
        <activity android:name=".ui.ProductDetailsActivity" />
        <activity android:name=".SettinsActivity" />
        <activity
            android:name=".NaviationDrawerActivity"
            android:label="@string/title_activity_naviation_drawer"
            android:theme="@style/Theme.SellTech.NoActionBar" />
        <activity
            android:name=".DashboardActivity"
            android:label="@string/title_activity_dashboard" />
        <activity android:name=".AddProductActivity" />
        <activity android:name=".SeeProductsActivity" />
        <activity android:name=".UserProfileActivity" />
        <activity android:name=".SnackbarActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomeScreenActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>