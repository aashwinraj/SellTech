<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_shop">

    <fragment
        android:id="@+id/navigation_shop"
        android:name="com.example.selltech.ui.home.ShopFragment"
        android:label="Shop"
        tools:layout="@layout/fragment_shop" />

    <fragment
        android:id="@+id/navigation_my_products"
        android:name="com.example.selltech.ui.dashboard.MyProductsFragment"
        android:label="My Products"
        tools:layout="@layout/fragment_my_products" />

    <fragment
        android:id="@+id/navigation_orders"
        android:name="com.example.selltech.ui.notifications.OrdersFragment"
        android:label="Orders"
        tools:layout="@layout/fragment_orders" />
